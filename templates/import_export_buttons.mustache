{{!
    @template mod_openstudio/logo

    Logo template of the OpenStudio UI.

    Classes required for JS:
    * none

    Data attributes required for JS:
    * none

    Example context (json):
    {
        "importlink": "http://example.com/mod/openstudio/import.php?id=13",
        "exportlink": "#"
    }

}}
<div class="openstudio-import-export-buttons">
    {{#importlink}}
        <a id="osep-bottombutton-import" href="{{importlink}}">
            {{#pix}} i/import, core, {{/pix}}
            <span> {{#str}} openstudio:import, mod_openstudio {{/str}} </span>
        </a>
    {{/importlink}}
    {{#exportlink}}
        <a id="osep-bottombutton-export" href="{{exportlink}}">
            {{#pix}} i/export, core, {{/pix}}
            <span> {{#str}} openstudio:export, mod_openstudio {{/str}} </span>
        </a>
    {{/exportlink}}
</div>